{
    "manifest_version": 3,
    "name": "ASL to English Translator",
    "version": "1.0",
    "description": "catch the ASL translate to english",
    "permissions": [
      "tabCapture",
      "activeTab",
      "scripting",
      "storage"
    ],
    "host_permissions": ["*://meet.google.com/*"],
    "background": {
      "service_worker": "dist/background.js"
    },
    "action": {
      "default_popup": "src/ui/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://meet.google.com/*"],
        "js": ["dist/contentScript.js"]
      }
    ]
  }
  